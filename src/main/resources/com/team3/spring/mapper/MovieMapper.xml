<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team3.spring.mapper.MovieMapper">
	<select id="getList" resultType="com.team3.spring.vo.MovieVO">
		SELECT * FROM M_MOVIE
	</select>
	
	<select id="getMovie" resultType="com.team3.spring.vo.MovieVO">
		SELECT * FROM M_MOVIE WHERE ID = #{id}
	</select>
	
</mapper>